<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.effectivenavigation">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.google.firebase.quickstart.effectivenavigation.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme" />
        <activity android:name="com.google.firebase.quickstart.effectivenavigation.NewPostActivity" />
        <activity android:name="com.google.firebase.quickstart.effectivenavigation.SignInActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.google.firebase.quickstart.effectivenavigation.PostDetailActivity" />
        <!-- [START firebase_iid_service] -->
        <service android:name="com.google.firebase.quickstart.effectivenavigation.notification_service.MyFirebaseInstanceIDService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>

        <activity
            android:name="com.google.firebase.quickstart.effectivenavigation.reports.DailyIncome"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar"
            >

        </activity>
    </application>

</manifest>